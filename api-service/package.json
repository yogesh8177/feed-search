{
  "name": "feed-search",
  "version": "1.0.0",
  "description": "This is a `POC` for searching and sorting feed data along with pagination",
  "main": "server.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "postbuild": "cp -r ./node_modules ./dist/api-service/node_modules",
    "start:test": "npm run build && NODE_ENV=test node ./dist/api-service/app.js",
    "start:prod": "npm run build && NODE_ENV=production node ./dist/api-service/app.js",
    "start:docker": "npm run build && NODE_ENV=docker node ./dist/api-service/app.js",
    "test": "npm run build && mocha ./dist/api-service/tests/*.test.js",
    "coverage": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yogesh8177/feed-search.git"
  },
  "author": "Yogesh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yogesh8177/feed-search/issues"
  },
  "homepage": "https://github.com/yogesh8177/feed-search#readme",
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "tslint": "^5.12.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "aws-sdk": "^2.771.0",
    "dotenv": "^8.2.0"
  }
}
